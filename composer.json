{
    "name": "shapecode/bitstamp-api",
    "description": "Bitstamp REST API php client",
    "keywords": [
        "bitstamp",
        "rest",
        "shapecode",
        "api"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/shapecode/twig-string-loader-bundle",
    "support": {
        "email": "support@shapecode.de",
        "issues": "https://github.com/shapecode/twig-string-loader-bundle/issues",
        "source": "https://github.com/shapecode/twig-string-loader-bundle/releases",
        "wiki": "https://github.com/shapecode/twig-string-loader-bundle/wiki"
    },
    "authors": [
        {
            "name": "Nikita Loges",
            "homepage": "http://nikita-loges.de",
            "email": "develop@nikita-loges.de"
        }
    ],
    "require": {
        "php": "^7.1",

        "doctrine/annotations": "^1.4",

        "symfony/serializer": "~2.8|~3.0|~4.0",
        "symfony/options-resolver": "~2.8|~3.0|~4.0",
        "symfony/property-access": "~2.8|~3.0|~4.0",

        "guzzlehttp/guzzle": "^6.3",
        "doctrine/collections": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0|~6.0",
        "symfony/debug": "~2.8|~3.0|~4.0",
        "symfony/var-dumper": "~2.8|~3.0|~4.0"
    },
    "autoload": {
        "psr-4": {
            "Shapecode\\Bitstamp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shapecode\\Bitstamp\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
